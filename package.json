{
  "name": "soldash-prismaql",
  "scripts": {
    "dev": "ts-node-dev --no-notify --respawn --transpile-only src/index.ts",
    "prisma-build": "npx prisma generate",
    "prisma-push": "npx prisma migrate dev",
    "prisma-push-prod": "npx prisma migrate deploy",
    "prisma-seed": "npx prisma db seed --preview-feature",
    "docker": "docker-compose up -d",
    "build": "tsc",
    "prod": "node dist/index.js",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@graphql-codegen/cli": "^1.21.7",
    "@graphql-codegen/introspection": "^1.18.2",
    "@graphql-codegen/typescript": "^1.23.0",
    "@graphql-codegen/typescript-operations": "^1.18.4",
    "@prisma/client": "^2.27.0",
    "@solana/spl-token-registry": "^0.2.203",
    "@solana/web3.js": "^1.22.0",
    "@types/graphql-fields": "^1.3.4",
    "apollo-server": "2.25.2",
    "class-validator": "0.13.1",
    "coingecko-api-v3": "0.0.12",
    "cors": "^2.8.5",
    "graphql": "15.5.1",
    "graphql-fields": "^2.0.3",
    "graphql-scalars": "^1.10.0",
    "prisma": "2.27.0",
    "reflect-metadata": "0.1.13",
    "type-graphql": "1.1.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.21.7",
    "@graphql-codegen/introspection": "1.18.2",
    "@graphql-codegen/typescript": "1.23.0",
    "@graphql-codegen/typescript-react-apollo": "^2.3.1",
    "@graphql-codegen/typescript-resolvers": "^1.20.0",
    "@types/coingecko-api": "^1.0.1",
    "@types/node": "12.20.16",
    "@types/ws": "7.4.6",
    "ts-node": "10.1.0",
    "ts-node-dev": "1.1.8",
    "typegraphql-prisma": "^0.14.7",
    "typescript": "4.3.5"
  },
  "engines": {
    "node": ">=14.8.0",
    "npm": ">=6.0.0"
  }
}
