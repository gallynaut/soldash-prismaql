{
  "name": "soldash-prismaql",
  "repository": {
    "type": "git",
    "url": "https://github.com/gallynaut/soldash-prismaql.git"
  },
  "scripts": {
    "dev": "ts-node-dev --respawn --no-notify --transpile-only src/index.ts",
    "start": "tsc && node dist/index.js",
    "prisma-build": "npx prisma generate",
    "prisma-push": "npx prisma migrate dev",
    "prisma-push-prod": "npx prisma migrate deploy",
    "prisma-seed": "npx prisma db seed --preview-feature",
    "docker": "docker-compose up -d",
    "build": "tsc",
    "prod": "node dist/index.js",
    "codegen": "graphql-codegen --config codegen.yml",
    "format": "npx prettier --write .",
    "lint": "npx eslint . --ext .js,.ts"
  },
  "dependencies": {
    "@prisma/client": "^2.27.0",
    "@solana/spl-token-registry": "^0.2.206",
    "@solana/web3.js": "^1.22.0",
    "apollo-server": "3.1.1",
    "class-validator": "0.13.1",
    "coingecko-api-v3": "0.0.12",
    "cors": "^2.8.5",
    "graphql": "15.5.1",
    "graphql-fields": "^2.0.3",
    "graphql-scalars": "^1.10.0",
    "reflect-metadata": "0.1.13"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.21.7",
    "@graphql-codegen/introspection": "1.18.2",
    "@graphql-codegen/typescript": "1.23.0",
    "@graphql-codegen/typescript-operations": "^1.18.4",
    "@graphql-codegen/typescript-react-apollo": "^2.3.1",
    "@graphql-codegen/typescript-resolvers": "^1.20.0",
    "@types/coingecko-api": "^1.0.2",
    "@types/graphql": "^14.5.0",
    "@types/graphql-bigint": "^1.0.0",
    "@types/graphql-fields": "^1.3.4",
    "@types/node": "16.4.10",
    "@types/ws": "7.4.7",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "graphql-bigint": "^1.0.0",
    "prettier": "^2.3.2",
    "prisma": "^2.27.0",
    "ts-node": "10.1.0",
    "ts-node-dev": "1.1.8",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^9.0.0",
    "type-graphql": "1.1.1",
    "typed-graphql": "^1.0.2",
    "typegraphql-prisma": "^0.14.7",
    "typescript": "^4.3.5",
    "typescript-eslint": "0.0.1-alpha.0"
  },
  "engines": {
    "node": ">=14.8.0",
    "npm": ">=6.0.0"
  }
}
