generator client {
  provider = "prisma-client-js"
  output   = "./generated/prisma-client-js"
}

generator typegraphql {
  provider = "typegraphql-prisma"
  output   = "./generated/typegraphql-prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Token {
  id            Int                         @default(autoincrement()) @id
  name          String @db.VarChar(32) @unique
  symbol        String @db.VarChar(16) @unique
  description   String? @db.VarChar(256)
  sol_address   String? @db.VarChar(64)
  gecko_id      String? @db.VarChar(64)
  website       String? @db.VarChar(256)
  twitter       String? @db.VarChar(256)
}
